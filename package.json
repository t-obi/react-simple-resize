{
  "name": "react-simple-resize",
  "version": "1.0.1",
  "description": "A react component rendering a resizable box",
  "keywords": [
    "react",
    "component",
    "resize",
    "resizable"
  ],
  "homepage": "http://t-obi.github.io/react-simple-resize/",
  "bugs": "https://github.com/t-obi/react-simple-resize/issues",
  "repository": "github:t-obi/react-simple-resize",
  "scripts": {
    "start": "./build_examples.js --watch",
    "build-examples": "./build_examples.js",
    "build": "babel lib --out-dir dist",
    "test": "karma start --single-run",
    "eslint": "eslint lib examples",
    "eslint-fix": "eslint --fix lib examples",
    "gh-pages": "git subtree push --prefix dist_examples origin gh-pages",
    "version": "npm run build && git add -A dist"
  },
  "author": "Tobias Kloht",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "directories": {
    "lib": "./lib",
    "example": "./examples"
  },
  "main": "dist/Resize",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-stage-1": "6.13.0",
    "css-loader": "0.23.1",
    "eslint": "3.3.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.0.0",
    "express": "4.14.0",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-router": "2.6.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "source-map-support": "0.4.2",
    "style-loader": "0.13.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
